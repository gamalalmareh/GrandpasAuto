{
  "version": 1,
  "framework": "custom",
  "routes": [
    {
      "path": "/api/*",
      "target": "compute",
      "status": 200,
      "kind": "Compute",
      "name": "default"
    },
    {
      "path": "/uploads/*",
      "target": "compute",
      "status": 200,
      "kind": "Compute",
      "name": "default"
    },
    {
      "path": "/assets/*",
      "target": "static",
      "status": 200,
      "kind": "Static"
    },
    {
      "path": "/*",
      "target": "static",
      "status": 200,
      "kind": "Static"
    }
  ],
  "computeResources": [
    {
      "name": "default",
      "runtime": "nodejs20.x",
      "entrypoint": "index.js"
    }
  ]
}
